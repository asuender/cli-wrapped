{
	"name": "cli-wrapped",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "nodemon --no-stdin --watch src --ext ts,tsx --exec tsx src/cli.tsx",
		"record": "vhs assets/demo.tape",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
		"prepare": "husky"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"fullscreen-ink": "^0.1.0",
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-spinner": "^5.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"yaml": "^2.8.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^25.0.3",
		"@types/react": "^18.0.32",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"nodemon": "^3.1.11",
		"prettier": "^3.7.4",
		"tsx": "^4.21.0",
		"typescript": "^5.0.3",
		"typescript-eslint": "^8.51.0"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	}
}
